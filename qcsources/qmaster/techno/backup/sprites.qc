void () info_sprite_static = {	if ( (self.model == string_null) ) {		return;	}	precache_model (self.model);	setmodel (self,self.model);	if ( (self.angles == '0.000 -1.000 0.000') ) {		self.angles = '-90.000 0.000 0.000';	} else {		if ( (self.angles == '0.000 -2.000 0.000') ) {			self.angles = '90.000 0.000 0.000';		}	}	makevectors (self.angles);	setorigin (self,(self.origin - (v_forward * 0.200)));	makestatic (self);};void () wallsprite = {   if ( !self.model ) {      self.model = "progs/s_blood1.spr";   }   precache_model (self.model);   setmodel (self,self.model);   if ( (self.angles == '0.000 -1.000 0.000') ) {      self.angles = '-90.000 0.000 0.000';   } else {      if ( (self.angles == '0.000 -2.000 0.000') ) {         self.angles = '90.000 0.000 0.000';      }   }   makevectors (self.angles);   setorigin (self,(self.origin - (v_forward * 0.200)));   makestatic (self);};